<%# Ansible inventory file -%>
<% @hosts.each do | host, parameters | -%>
<%= host -%>
<% parameters.each do | key, value | -%>
 <%= key -%>=<%= value -%>
<% end -%>
<%# should be new line %>
<% end -%>
<%# should be new line %>
<% @groups.each do | group_name, group_list | -%>
[<%= group_name -%>]
<% group_list.each do | group_element | -%>
<%= group_element %>
<% end %>
<% end %>
